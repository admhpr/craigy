    "craiglist",
    "node"
  ],
  "author": "Adam Hapur",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/harps116/craigy/issues"
  },
  "homepage": "https://github.com/harps116/craigy#readme",
  "dependencies": {
    "chalk": "^2.4.2",
    "dotenv": "^7.0.0",
    "nightmare": "^3.0.1",
    "nightmare-upload": "^0.1.1",
    "tar": "^4.4.8"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.2.0",
    "mocha": "^6.1.4",
    "prettier": "^1.17.0"
  }
}
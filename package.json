{
  "name": "craigy",
  "version": "1.0.0",
  "description": "A program to post items for sale to Craignpmslist",
  "main": "index.js",
  "scripts": {
    "start": "node ./src/index.js",
    "lint": "eslint './src/**/*.js'",
    "format": "prettier --write 'src/**/*.js' 'src/*.js'",
    "test": "mocha"
  },
  "husky": {
